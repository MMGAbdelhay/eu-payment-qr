{
  "name": "eu-payment-qr",
  "version": "1.0.0",
  "description": "Generate and parse EPC QR codes (GiroCode) for SEPA payments",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "epc",
    "qr",
    "girocode",
    "sepa",
    "payment",
    "iban",
    "germany",
    "europe",
    "invoice"
  ],
  "author": "MMGAbdelhay",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MMGAbdelhay/eu-payment-qr"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
